<C:calendar-query xmlns:D="DAV:" xmlns:C="urn:ietf:params:xml:ns:caldav">
    <D:prop>
        <C:calendar-data/>
        <D:getetag/>
    </D:prop>
    <C:filter>
        <C:comp-filter name="VCALENDAR">
        	<!--test="anyof" ne semble pas supportÃ© par le serveur caldav nextcloud-->
            <C:comp-filter name="VTODO">
                <C:prop-filter name="STATUS" test="anyof">
            		<C:text-match negate-condition="yes">CANCELLED</C:text-match>
            		<C:is-not-defined/>
            	</C:prop-filter>
            	<C:prop-filter test="anyof">
			    	<C:prop-filter name="COMPLETED">
			    		<C:is-not-defined/>
			    	</C:prop-filter>
			    	<C:time-range start="<%= start %>"></C:time-range>
			    </C:prop-filter>
            </C:comp-filter>
        </C:comp-filter>
    </C:filter>
</C:calendar-query>
